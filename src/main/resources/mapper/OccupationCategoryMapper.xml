<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qingming.jobserver.mapper.OccupationCategoryMapper">

    <select id="selectByName" resultType="com.qingming.jobserver.model.entity.OccupationCategory">
        SELECT *
        FROM occupation_category
        WHERE name = #{name}
    </select>

</mapper> 